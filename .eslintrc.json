{
  "extends": [
    "react-app"
  ],
  "rules": {
    "semi": ["error", "never"],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "max-len": [ "warn", {
      "code": 120,
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreRegExpLiterals": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": false
    }]
  }
}
